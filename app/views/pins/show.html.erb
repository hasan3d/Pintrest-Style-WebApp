 <%= link_to 'Back', pins_path %>

<div class="showimg">
    <%= image_tag @pin.image.url(:medium)%>
</div>
    <p class="description">
      <%= @pin.description %>
    </p>
    
    <p>
      <strong>
        Uploaded by <%= @pin.user.name %>
      </strong>
      
    </p>
    <% if current_user == @pin.user %>
      <%= link_to edit_pin_path(@pin) do %> 
<span class="glyphicon glyphicon-edit" aria-hidden="true"></span> Edit
      <% end %>
              <% end %>  


